{
     "env": {
          "browser": true,
          "es2021": true
     },
     "extends": ["airbnb-base"],
     "parser": "@typescript-eslint/parser",
     "parserOptions": {
          "ecmaVersion": "latest",
          "sourceType": "module"
     },
     "plugins": ["@typescript-eslint"],
     "rules": {
          "indent": "off",
          "padded-blocks": "off",
          "import/no-unresolved": "off",
          "import/extensions": "off",
          "class-methods-use-this": "off",
          "no-console": "off"
     }
}
