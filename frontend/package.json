{
  "name": "raincounter",
  "version": "0.1.0",
  "description": "front end for raingauge and weather app",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint 'src/**/*.ts'",
    "build": "babel --out-file build/bundle.js  src/**/*.ts",
    "test": "test",
    "docker-up": "cd $INIT_CWD && cd ../docker && docker-compose up --build --remove-orphans",
    "docker-down": "cd $INIT_CWD && cd ../docker && docker-compose down",
    "psql": "psql -h localhost -U postgres raincounter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntbloom/raincounter.git"
  },
